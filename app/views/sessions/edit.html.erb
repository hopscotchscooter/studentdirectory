<h1>Edit Student</h1>
<% if flash[:notice] %>
<div class="notice"><%= flash[:notice] %></div>
<% end %>
<br>
<%= form_for :user, url: users_path do |f| %>
 <p>
   <%= f.label :name %><br>
   <%= f.text_field :name %>
 </p>
 <p>
   <%= f.label :nickname %><br>
   <%= f.text_field :nickname %>
 </p>
 <p>
   <%= f.label :email %><br>
   <%= f.text_field :email %>
 </p>
  <p>
     <%= f.label :password %><br>
     <%= f.password_field :password %><br>
  </p>

   <p>
     <%= f.label :password_confirmation %><br>
     <%= f.password_field :password_confirmation %><br>
   </p>

 
 <p>
   <%= f.submit %>
 </p>
<% end %>
<%= link_to 'Return to main page', sessions_path %>