<h1>Edit Student</h1>

<br>
<%= form_for :user, url: user_path(@user), method: :patch do |f| %>
 <p>
   <%= f.label :name %><br>
   <%= f.text_field :name %>
 </p>
 <p>
   <%= f.label :nickname %><br>
   <%= f.text_field :nickname %>
 </p>
 <p>
   <%= f.label :email %><br>
   <%= f.text_field :email %>
 </p>
  <p>
     <%= f.label :password %><br>
     <%= f.password_field :password %><br>
  </p>

   <p>
     <%= f.label :password_confirmation %><br>
     <%= f.password_field :password_confirmation %><br>
   </p>

 
 <p>
   <%= f.submit "Update" %>
 </p>
<% end %>
<%= link_to 'Return to main page', users_path %>